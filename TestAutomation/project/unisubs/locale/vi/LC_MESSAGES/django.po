#: templates/editor/dialogs.html:11
msgid "Subtitle line length"
msgstr "Độ dài dòng phụ đề"

#: templates/editor/dialogs.html:12
msgid "Keep subtitle length to about 42 characters;"
msgstr "Để độ dài phụ đề trong khoảng 42 ký tự;"

#: templates/editor/dialogs.html:13
msgid "avoid more than 2 lines per subtitle."
msgstr "tránh nhiều hơn 2 dòng trên một phụ đề."

#: templates/editor/dialogs.html:16
msgid "Minimum duration"
msgstr "Khoảng thời gian tối thiểu"

#: templates/editor/dialogs.html:17
msgid "Subtitles should not be less than 1 second."
msgstr "Phụ đề không được ngắn hơn 1 giây."

#: templates/editor/dialogs.html:20
msgid "Maximum duration"
msgstr "Khoảng thời gian tối đa"

#: templates/editor/dialogs.html:21
msgid "Split subtitles lasting more than 7 seconds."
msgstr "Chia các phụ đề sao cho không dài quá 7 giây."

#: templates/editor/dialogs.html:24
msgid "Reading speed"
msgstr "Tốc độ đọc"

#: templates/editor/dialogs.html:25
msgid "8-25 characters/second is recommended;"
msgstr "Đề xuất 8-25 ký tự/giây;"

#: templates/editor/dialogs.html:26
msgid "8-16 characters/second for children."
msgstr "8-16 ký tự/giây cho trẻ em."

#: templates/editor/dialogs.html:29
msgid "Sounds"
msgstr "Âm thanh"

#: templates/editor/dialogs.html:30
msgid "Describe meaningful sounds that are relevant to the plot."
msgstr "Miêu tả âm thanh có nghĩa liên quan đến cốt truyện."

#: templates/editor/dialogs.html:31
msgid "ex: (water dripping)"
msgstr "ví dụ: (nước nhỏ giọt)"

#: templates/editor/dialogs.html:34
msgid "Music"
msgstr "Âm nhạc"

#: templates/editor/dialogs.html:35
msgid "Describe relevant music which does not have relevant lyrics."
msgstr "Miêu tả âm nhạc liên quan nhưng không có lời nhạc."

#: templates/editor/dialogs.html:36
msgid "ex: ♪ (jazz music) ♪"
msgstr "ví dụ: ♪ (nhạc jazz) ♪"

#: templates/editor/dialogs.html:39
msgid "Speaker Identification"
msgstr "Nhận dạng người nói"

#: templates/editor/dialogs.html:40
msgid "Use parenthesis to indicate when someone is speaking off-screen."
msgstr "Sử dụng dấu ngoặc đơn để biểu thị ai đó đang nói ngoài màn hình."

#: templates/editor/dialogs.html:41 templates/editor/dialogs.html:79
msgid "ex: (Michael) Wait for me!"
msgstr "ví dụ: (Michael) Đợi mình với!"

#: templates/editor/dialogs.html:44
msgid "Show Advanced Guidelines"
msgstr "Hiển thị Hướng dẫn Chi tiết"

#: templates/editor/dialogs.html:45
msgid "Hide Advanced Guidelines"
msgstr "Ẩn Hướng dẫn Chi tiết"

#: templates/editor/dialogs.html:49
msgid "Translate the whole idea"
msgstr "Dịch toàn bộ ý tưởng"

#: templates/editor/dialogs.html:50
msgid "Translations don't need to be word for word. Look at the context."
msgstr "Bản dịch không được dịch từng từ một. Hãy nhìn vào nội dung của nó."

#: templates/editor/dialogs.html:51
msgid ""
"Make sure you are using the correct punctuation for the whole unit of text."
msgstr "Hãy đảm bảo bạn đang sử dụng đúng dấu chấm câu cho cả đoạn văn."

#: templates/editor/dialogs.html:55
msgid "Maximum timing offset"
msgstr "Khoảng trống thời gian tối đa"

#: templates/editor/dialogs.html:56
msgid ""
"Subtitles shouldn't start more than 0.5 seconds before or after the audio "
"begins."
msgstr "Phụ đề không nên bắt đầu hơn 0,5 giây trước hoặc sau khi âm thanh bắt đầu."

#: templates/editor/dialogs.html:59
msgid "On-screen texts"
msgstr "Văn bản trên màn hình"

#: templates/editor/dialogs.html:60
msgid "Translate the texts on the video that are relevant to the plot."
msgstr "Dịch văn bản trên video liên quan đến cốt truyện."

#: templates/editor/dialogs.html:63
msgid "Use meaningful speech"
msgstr "Sử dụng câu nói có nghĩa"

#: templates/editor/dialogs.html:64
msgid "Exclude things like “um” “ah” and other disfluencies."
msgstr "Loại bỏ những thứ như \"ừm\" \"à\" và nhiều câu ngắc ngứ khác."

#: templates/editor/dialogs.html:67
msgid "Indicate inaudible parts"
msgstr "Biểu thị những phần không nghe thấy được"

#: templates/editor/dialogs.html:68
msgid "If the audio is inaudible, it should be marked as [inaudible]."
msgstr "Nếu âm thanh không nghe được, nên đánh dấu lại như [không nghe được]."

#: templates/editor/dialogs.html:71
msgid "Lyrics"
msgstr "Lời nhạc"

#: templates/editor/dialogs.html:72
msgid "Wrap relevant lyrics in musical notes."
msgstr "Cho lời nhạc liên quan vào trong các nốt nhạc."

#: templates/editor/dialogs.html:73
msgid "ex ♪ Tell me dear Billy, ♪"
msgstr "ví dụ ♪ Hãy nói với em, Billy yêu dấu, ♪"

#: templates/editor/dialogs.html:77
msgid "Speaker identification"
msgstr "Nhận dạng người nói"

#: templates/editor/dialogs.html:78
msgid "Identify off-screen and unclear speakers in parentheses."
msgstr "Xác minh người nói ngoài màn hình và người nói không rõ ràng trong dấu ngoặc."

#: templates/editor/dialogs.html:83
msgid "Foreign language"
msgstr "Ngôn ngữ khác"

#: templates/editor/dialogs.html:84
msgid "Identify relevant speech in foreign language. "
msgstr "Xác định câu nói liên quan trong ngôn ngữ khác."

#: templates/editor/dialogs.html:85
msgid "ex: (Russian): Thank you."
msgstr "ví dụ: (Tiếng Nga): Cảm ơn."

#: templates/editor/dialogs.html:89
msgid "Simultaneous dialogue"
msgstr "Đối thoại đồng thời"

#: templates/editor/dialogs.html:90
msgid "Used exclusively when two speakers talk in the same subtitle. "
msgstr "Được sử dụng đặc biệt khi 2 người cùng nói trong một phụ đề."

#: templates/editor/dialogs.html:91
msgid ""
"Each speaker should have one line, and both lines should have an hyphen. "
msgstr "Mỗi người nói để trong một dòng, và cả hai dòng đều có dấu gạch nối."

